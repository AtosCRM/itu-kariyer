<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<managed-bean>
		<managed-bean-name>subscriber</managed-bean-name>
		<managed-bean-class>com.atos.subscriber.model.Subscriber</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>subscriberAct</managed-bean-name>
		<managed-bean-class>com.atos.subscriber.action.SubscriberAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<application>
		<resource-bundle>
			<base-name>com.atos.messages</base-name>
			<var>labelDes</var>
		</resource-bundle>
	</application>

</faces-config>